{
  "arrowParens": "always",
  "bracketSameLine": false,
  "bracketSpacing": true,
  "embeddedLanguageFormatting": "auto",
  "htmlWhitespaceSensitivity": "css",
  "insertPragma": false,
  "jsxSingleQuote": false,
  "printWidth": 120,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "requirePragma": false,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 4,
  "trailingComma": "all",
  "useTabs": false,
  "vueIndentScriptAndStyle": false,
  "parser": "typescript",
  "endOfLine": "auto",
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "tabWidth": 2
      }
    },
    {
      "files": "*.md",
      "options": {
        "proseWrap": "always"
      }
    },
    {
      "files": "*.html",
      "options": {
        "printWidth": 80
      }
    },
    {
      "files": [
        "*.yml",
        "*.yaml"
      ],
      "options": {
        "tabWidth": 2,
        "singleQuote": false,
        "proseWrap": "preserve"
      }
    },
    {
      "files": "*.css",
      "options": {
        "singleQuote": false,
        "printWidth": 80
      }
    },
    {
      "files": "*.scss",
      "options": {
        "printWidth": 80,
        "bracketSpacing": false
      }
    }
  ]
}